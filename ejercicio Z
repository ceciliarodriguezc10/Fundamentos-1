#include <iostream>
#include "math.h"
#include "conio.h"

using namespace std;

int main (){
	int n=0;
	double mult=1;
	cout<<"Ingrese cuantos valores quiere calcular: "<<endl; 
	cin>>n;
	
	for(int i=1;i<=n;i++){
		if (i %2==0){
			mult= mult*i;
		}
	}
	cout<<"La multiplicacion de los pares hasta "<<n<<"es: "<<mult<<endl;
	
	return 0;
}
